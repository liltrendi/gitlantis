{
  "name": "gitlantis",
  "displayName": "Gitlantis",
  "main": "./out/index.js",
  "engines": {
    "vscode": "^1.80.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "gitlantis.openWebview",
        "title": "Gitlantis"
      }
    ]
  },
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:webview": "tsc -b && vite build",
    "build:extension": "tsc -p tsconfig.extension.json",
    "build": "npm run build:webview && npm run build:extension",
    "watch": "tsc -watch -p tsconfig.extension.json"
  },
  "dependencies": {
    "@react-three/cannon": "^6.6.0",
    "@react-three/drei": "^9.82.0",
    "@react-three/fiber": "^8.14.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^6.1.18",
    "three": "^0.156.1",
    "three-stdlib": "^2.25.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.24",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/three": "^0.176.0",
    "@types/vscode": "^1.80.0",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  },
  "activationEvents": [
    "onCommand:gitlantis.openWebview",
    "onStartupFinished"
  ]
}
